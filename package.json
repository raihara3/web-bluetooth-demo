{
  "name": "virtual-koten",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-scss": "node-sass ./src/scss/ --output ./dist/",
    "watch-scss": "node-sass --watch ./src/scss/ --output ./dist/",
    "webpack": "webpack --config webpack.config.js --mode development",
    "start": "run-p webpack build-scss watch-scss webpackdevserver",
    "precommit": "yarn run eslint .eslintrc",
    "webpackdevserver": "webpack-dev-server --https",
    "create-ssl-certificate": "run-s create-ca-privatekey create-ca-csr create-ca-crt create-server-privatekey create-server-csr create-server-crt",
    "create-ca-privatekey": "openssl genrsa -out ca-privatekey.pem 2048",
    "create-ca-csr": "openssl req -new -key ca-privatekey.pem -out ca-csr.pem",
    "create-ca-crt": "openssl req -x509 -key ca-privatekey.pem -in ca-csr.pem -out ca-crt.pem -days 3560",
    "create-server-privatekey": "openssl genrsa -out server-privatekey.pem",
    "create-server-csr": "openssl req -new -key server-privatekey.pem -out server-csr.pem",
    "create-server-crt": "openssl x509 -req -CA ca-crt.pem -CAkey ca-privatekey.pem -CAcreateserial -in server-csr.pem -out server-crt.pem -days 3650"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raihara3/virtual-koten.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/raihara3/virtual-koten/issues"
  },
  "homepage": "https://github.com/raihara3/virtual-koten#readme",
  "dependencies": {
    "@types/web-bluetooth": "^0.0.9",
    "husky": "^4.3.0",
    "three": "^0.121.1",
    "ts-loader": "^8.0.11",
    "typescript": "^4.0.5",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "copy-webpack-plugin": "^6.2.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  }
}
